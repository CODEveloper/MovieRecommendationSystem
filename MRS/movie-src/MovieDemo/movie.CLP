(defrule MAIN::create-con
	(declare (salience 10))
	(movie (title ?title) (rating ?rating))
	=>
	(assert (movie-con (title ?title) (matched "") (rating ?rating)))
	(seed (round (time)))
)

	

;;;;;;;;;;;;;;;;;;;FUNCTION
	
(deffunction MAIN::movie-modify-con (?title ?con-value ?matched)
	(bind ?facts (find-all-facts ((?f movie-con)) (eq ?f:title ?title)))
	(bind ?m (nth$ 1 ?facts))
	(modify ?m (con (+ (fact-slot-value ?m con) ?con-value)) (matched (fact-slot-value ?m matched) ?matched)))

(deffunction MAIN::movie-modify-con-no (?title ?con-value)
	(bind ?facts (find-all-facts ((?f movie-con)) (eq ?f:title ?title)))
	(bind ?m (nth$ 1 ?facts))
	(modify ?m (con (- (fact-slot-value ?m con) ?con-value))))

(deffunction MAIN::movie-modify-con-zero (?title)
	(bind ?facts (find-all-facts ((?f movie-con)) (eq ?f:title ?title)))
	(bind ?m (nth$ 1 ?facts))
	(modify ?m (con 0)))

(deffunction MAIN::get-random ()
	(printout t "im here" (time))
	(seed (round (time)))
	(bind ?facts (find-all-facts ((?f movie)) TRUE))
	(bind ?m (nth$ (random 1 (length$ ?facts)) ?facts))
	)
	;;;;(modify ?m (con 1)))
	

(deffunction MAIN::movie-sort (?m1 ?m2)
   (or (< (fact-slot-value ?m1 con)
          (fact-slot-value ?m2 con))
       (and (= (fact-slot-value ?m1 con)
               (fact-slot-value ?m2 con))
            (< (fact-slot-value ?m1 rating)
               (fact-slot-value ?m2 rating)))))

(deffunction MAIN::get-movie-list ()
  (bind ?facts (find-all-facts ((?f movie-con))
                               (> ?f:con 0)))
  (sort movie-sort ?facts))

(deffunction MAIN::get-movie-theatre-list ()
  (bind ?facts (find-all-facts ((?f movie-con))
                               (> ?f:con 0)))
  (sort movie-sort ?facts))
  
 (deffunction MAIN::get-movie-by-title (?title)
	(bind ?facts (find-all-facts ((?f movie)) (eq ?f:title ?title)))
	(bind ?m (nth$ 1 ?facts)))
  
  

;;;;;;;;;;;;;;;;;;;YEAR

(defrule MAIN::get-input-year
	(preference (name year) (value ?year))
	(movie (title ?title) (year ?year))
=>
	(movie-modify-con ?title 1 year))

(defrule MAIN::get-input-no-year
	(preference (name no-year) (value ?year))
	(movie (title ?title) (year ?year))
=>
	(movie-modify-con-no ?title 1))
  
  

;;;;;;;;;;;;;;;;;;;LOCATION

(defrule MAIN::get-input-location
	(preference (name location) (value ?location))
	(theatre (name ?theatre-title) (location ?location))
	(movie (title ?title) (place $? ?theatre-title $?) (mode theatre))
	(movie-con (title ?title) (matched ~location))
=>
	(movie-modify-con ?title 1 location)
)

(defrule MAIN::get-input-no-location
	(declare (salience -20))
	(preference (name location))
	(movie-con (title ?title) (con ?con-value) (matched ~location))
	(test(neq ?con-value 0))
=>
	(movie-modify-con-zero ?title)
)

