	(preference (name mode) (value theatre))
	(movie (title ?title) (mode DVD))
=>
	;(printout t ?title " DVD 0" crlf crlf crlf)
	(movie-modify-con-zero ?title)
)

(defrule MAIN::reset-theatre
	(declare (salience -10))
	(preference (name mode) (value DVD))
	(movie (title ?title) (mode theatre))
=>
	;(printout t ?title " theatre 0" crlf crlf crlf)
	(movie-modify-con-zero ?title)
)


	
	
